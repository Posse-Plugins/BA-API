<settings>
  <servers>
	  <server>
		  <id>ossrh</id>
		  <username>${{ secrets.OSSRH_USER }}</username>
		  <password>${{ secrets.OSSRH_TOKEN }}</password>
	  </server>
  </servers>
  <profiles>
	  <profile>
		  <id>ossrh</id>
		  <activation>
			  <activeByDefault>true</activeByDefault>
		  </activation>
		  <properties>
			  <gpg.executable>gpg2</gpg.executable>
			  <gpg.passphrase>${{ secrets.GPG_PHRASH }}</gpg.passphrase>
		  </properties>
	  </profile>
  </profiles>
</settings>
